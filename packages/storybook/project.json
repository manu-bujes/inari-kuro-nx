{
  "name": "storybook",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/storybook/src",
  "projectType": "library",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "start-storybook -p 6006"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "build-storybook"
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    }
  },
  "tags": []
}
